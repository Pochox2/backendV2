<section>
    {{#if user}}
<h1 class="">Hola {{user.first_name}}, Bienvenido a Quartz</h1>
    {{else}}
<h1>Bienvenido a Quartz</h1>
{{/if}}
<div class="">
    <div class="">
        {{#each resultado.docs}}
        
            <div class="">
                <div class="">
                <img src={{this.thumbnail}} class="" alt={{this.tittle}}>
                <h5 class="">{{this.tittle}}</h5>
                    <div class="">
                    <p class="">{{this.description}}</p>
                    <p class="">Precio:${{this.price}}</p>
                        <div class="">
                        <a href="/api/products/{{this.id}}" class="">Detalles</a>
                        </div>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
    <div class="">
        {{#if resultado.hasPrevPage}}
        <a class="" href="{{resultado.prevLink}}">Pagina anterior</a>
        {{else}}
        <button class="" disabled>Previous page</button>
        {{/if}}
            {{#if resultado.hasNextPage}}
            <a class="" href="{{resultado.nextLink}}">Siguiente pagina</a>
        {{else}}
            <button  class="" disabled>Siguiente pagina</button>
        {{/if}}
    </div>
</div>

</section>
<script src="/socket.io/socket.io.js"></script>
<script src="../../public/socket/ClienteSocket.js"></script>